{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/interface-name-prefix": ["error",{ "prefixWithI":"always"}],
      "no-unused-vars":"off",
      "camelcase":"off",
      "no-useless-constructor": "off"
    }
}
